@character set 'utf-8';
@import url(http://fonts.googleapis.com/earlyaccess/notosanskr.css);	
@import url(http://fonts.googleapis.com/earlyaccess/nanummyeongjo.css);

/**** reset ****/

* {
	margin: 0;
	padding: 0;
	line-height: 1;
	font-size: 100%;
}

li {
	list-style: none;
}

a {
	 text-decoration: none;
}

img {
	vertical-align: bottom;
	border: none 0;
}

h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
}

em {
	font-style: normal;
}

strong {
	font-weight: normal;
}

/**** common ****/

.wrapper {
	width: 1100px;
	margin: 0 auto;
	position: relative;
}

.clearfix::after {
	content: ' ';
	display: block;
	clear: both;
}

body {
	font-family: 'Noto Sans KR', sans-serif;
	color: #666;
	font-weight: 300;
	padding-top: 156px;
}

/**** main ****/

/** header **/

#header {
	position: absolute;
	top: 0;
	width: 100%;
	box-sizing: border-box;
	border-top: 6px solid #307676;
	background-image: url(../images/header_bg2.jpg);
	background-position: center top;
	height: 206px;
	/*overflow: hidden;*/
	
	/*초기값*/
	height: 156px;
	z-index: 9999;
}

#header .wrapper {
	box-sizing: border-box;
	padding: 15px 0 20px;
}

#header h1.logo {
	position: absolute;
	left: 50%;
	margin-left: -76px;
	z-index: 20;
}

#header nav.gnbWrap {
	text-align: center;
}

#header ul.gnb {
	width: 100%;
	position: absolute;
	top: 58px;
	left: 0;
	margin-left: 30px;
}

#header ul.gnb>li {
	display: inline-block;
	vertical-align: top;
	padding: 0 28px 32px;
	height: 58px;
	box-sizing: border-box;
}

#header ul.gnb>li:hover h2 a {
	color: #1c6972;
}

#header ul.gnb li h2 a {
	font-size: 18px;
	color: #444;
	font-weight: 400;
}

#header ul.gnb>li:nth-child(1) {
	padding-left: 0;
}

#header ul.gnb>li:nth-child(4) {
	margin-left: 318px;
}

#header ul.gnb>li:nth-child(6) {
	padding-right: 0;
}

#header ul.gnb>li ul.menu {
	position: absolute;
	top: 58px; left: 0;
	width: 100%;
	height: 50px;
	display: none;
}

#header ul.gnb>li ul.menu li {
	display: inline-block;
	background-image: url(../images/gnb_bar.png);
	background-position: left center;
	padding-left: 17px;
	background-repeat: no-repeat;
	margin: 0 5px;
}

#header ul.gnb>li ul.menu li:first-child {
	background: none;
}

#header ul.gnb>li ul.menu li a {
	line-height: 50px;
	color: #ccc;
	font-size: 14px;
	transition: all 0.1s;
}

#header ul.gnb>li ul.menu li a:hover {
	font-weight: 400;
	color: #1c6972;
}

#header nav.lnb {
	position: absolute;
	top: 0;
	right: 0;
}

#header nav.lnb ul.lnb li {
	display: inline-block;
	margin-left: 6px;
	height: 20px;
	line-height: 15px;
}

#header nav.lnb ul.lnb li.sitemap {
	background-color: #307676;
	border-radius: 0 0 10px 10px;
	background-image: url(../images/more.png);
	background-repeat: no-repeat;
	background-position: 55px center;
}

#header nav.lnb ul.lnb li.sitemap a {
	display: inline-block;
	width: 78px;
	height: 20px;
	color: #fff;
	padding: 0 13px;
	box-sizing: border-box;
	font-weight: 400;
}

#header nav.lnb ul.lnb li a {
	font-size: 11px;
	color: #666;
	text-transform: uppercase;
	font-weight: 400;
}


/** visual **/

#visual {
	height: 723px;
}

#visual div.slide {
	height: 723px;
	position: relative;
	text-align: center;
	width: 100%;
	color: #242424;
	background-size: cover;
	background-position: center;
}

#visual div.slide1 { background-image: url(../images/visual01.jpg); color: #fff; font-weight: 100;}
#visual div.slide2 { background-image: url(../images/visual02.jpg);}
#visual div.slide3 { background-image: url(../images/visual03.jpg);}

#visual div.wrapper div.copy {
	font-family: 'Nanum Myeongjo';
	position: absolute;
	top: 280px;
	width: 100%;
}

#visual div.wrapper div.copy h1 {
	font-size: 65px;
	letter-spacing: -5px;
}

#visual div.wrapper div.copy h2 {
	font-size: 30px;
	letter-spacing: -4px;
	margin-bottom: 12px;
}

#visual .cycle-pager {
	bottom: -180px;
}

/** container **/

/* container common */

#container {
	text-align: center;
}

#container h1.tit {
	font-size: 35px;
	padding-top: 100px;
	width: 100%;
	font-family: 'Nanum Myeongjo';
	color: #333;
	letter-spacing: -2px;
	font-weight: 600;
}

#container h1.tit::after {
	content: '';
	display: block;
	width: 48px;
	height: 3px;
	background-color: #751e12;
	margin: 16px auto;
}

#container p.desc {
	font-size: 18px;
	line-height: 1.3em;
}

/* top 버튼 */

#container p.top {
	position: fixed;
	right: 0;
	/*bottom: 200px;*/
	top: 0;
	opacity: 0.85;
	display: none;
	z-index: 25;
}

/* 검색 서비스 */

#container .search h1.tit {
	padding-top: 100px;
}

#container p.desc {
	margin-bottom: 53px;
}

#container form legend {
	display: none;
}

#container form fieldset {
	border: none 0;
}

#container .search span {
	display: inline-block;
    width: 655px;
    height: 74px;
    border: 4px solid #751e12;
	box-sizing: border-box;
}

#container .search input.inp {
	/*width: 400px;
	height: 66px;
	outline:2px solid #751e12;
	padding: 12px 20px;
    box-sizing: border-box;*/
	width: 620px;
    height: 66px;
    border: 0;
    font-size: 18px;
    color: #000;
    background-color: transparent;
    outline: none;
	font-family: 'nanumbarungothic';
	background-image: url(../images/tri.png);
	background-position: right center;
	background-repeat: no-repeat;
}

#container .search a.btn {
	display: inline-block;
	width: 60px;
    height: 74px;
	line-height: 74px;
    vertical-align: top;
	color: #fff;
	background-color: #751e12;
	font-size: 15px;
	margin-left: -6px;
	padding-right: 3px;
	font-weight: 500;
}

/* 문화유산정보 */

#container .info h1.tit {
	padding-top: 0;
}

#container .info h1.tit::before {
	content: '';
	display: block;
	width: 870px;
	height: 1px;
	background-color: #ddd;
	margin: 120px auto 100px;
}	

#container div.infoList div.item {
	margin: 10px;
}

#container div.infoList div a {
	color: #666;
}

#container div.infoList div img {
	width: 100%;
}

#container div.infoList div strong.name {
	margin: 34px 0 10px;
	font-size: 18px;
	font-weight: 400;
	color: #333;
}

#container div.infoList div strong.name,
#container div.infoList div span.txt {
	display: inline-block;
	text-align: justify;
	width: 100%;
}

#container div.infoList div span.txt {
	font-size: 15px;
	line-height: 1.5em;
	letter-spacing: -1px;
	/*font-weight: 400;*/
}

#container .info p.btn {
	position: absolute;
	top: 65%;
	width: 100%;
}

#container .info p.btn a.prev {
	float: left;
}

#container .info p.btn a.next {
	float: right;
}

/* 공지사항 */

#container .notice { 
	margin-top: 120px;
	background-color: #f2f2f2;
	height: 700px;
}

#container .notice ul.noticeTit>li {
	float: left;
}

#container .notice ul.noticeTit li h2 a {
	display: block;
	width: 220px;
	height: 44px;
	line-height: 42px;
	font-size: 20px;
	letter-spacing: -1px;
	border: 1px solid #8a9870;
	box-sizing: border-box;
	color: #576045;
	border-left: none;
	background-color: rgba(118,135,82,0);
	font-weight: 300;
}

#container .notice ul.noticeTit li h2 a.active {
	background-color: rgba(118,135,82,1);
	color: #fff;
}

#container .notice ul.noticeTit li h2 span {
	content: '';
	display: block;
	background-color: rgba(118,135,82,0);
	width: 20px;
	height: 20px;
	transform: rotate(45deg);
	margin: -11px auto 0;
}

#container .notice ul.noticeTit li h2 span.active {
	background-color: rgba(118,135,82,1);
}

#container .notice ul.noticeTit li:first-child h2 a {
	border-left: 1px solid #8a9870;
	border-radius: 50px 0 0 50px
}

#container .notice ul.noticeTit li:last-child h2 a {
	border-radius: 0 50px 50px 0;
}

#container .notice ul.noticeTit li ul.noticeCon {
	position: absolute;
	width: 1100px;
	border: 1px solid #8a9870;
	border-radius: 20px;
	padding: 14px 0;	
	font-size: 15px;
	margin-top: 27px;
}

#container .notice ul.noticeTit li ul.noticeCon {
	display: none;
	left: 0;
}

#container .notice ul.noticeTit li:first-child ul.noticeCon {
	display: block;
}

#container .notice ul.noticeTit li ul.noticeCon li a {
	display: inline-block;
	width: 973px;
	height: 63px;
	line-height: 63px;
	border-bottom: 1px dotted #ccc;
}

#container .notice ul.noticeTit li ul.noticeCon li {
	background-color: rgba(255,255,255,0);
	transition: all 0.2s;
}

#container .notice ul.noticeTit li ul.noticeCon li:hover {
	background-color: #fff;
}

#container .notice ul.noticeTit li ul.noticeCon li:last-child a {
	border: none;
}

#container .notice ul.noticeTit li ul.noticeCon li h3,
#container .notice ul.noticeTit li ul.noticeCon li p {
	display: inline-block;
	color: #444;
}

#container .notice ul.noticeTit li ul.noticeCon li p.date {
	width: 20%;
	color: #5c733c;
	vertical-align: middle;
}

#container .notice ul.noticeTit li ul.noticeCon li p.date em { 
	font-size: 20px;
	font-weight: 500;
	font-family: arial;
}

#container .notice ul.noticeTit li ul.noticeCon li p.date span {
	display: inline-block;
	width: 100%;
	font-size: 12px;
	font-family: arial;
}

#container .notice ul.noticeTit li ul.noticeCon li h3 {
	width: 10%;
}

#container .notice ul.noticeTit li ul.noticeCon li p {
	width: 69%;
}


/*
#container .notice ul.noticeTit li:nth-child(1) ul.noticeCon {
	display: inline-block;
}*/

/* 홍보영상 */

#container .video div.videoWrap {
	height: 600px;
	background-image: url(../images/video_preview_main.jpg);
	background-position: center;
	background-size: cover;
	position: relative;
}

#container .video p.play {
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -41px;
	margin-left: -60px;
}

#container .video {
	position: relative;
}

#container .video div.popupVid {
	background-color: rgba(0,0,0,0.7);
	position: fixed;
	top: 0;
	width: 100%;
	height: 100%;
	display: none;
	z-index: 10;
}

#container .video div.popupVid iframe {
	position: absolute;
	top: 50%;
	left: 50%;
	margin-top: -225px;
	margin-left: -400px;
}

#container .video div.popupVid p.close a {
	position: fixed;
	top: 50px;
	right: 50px;
	display: inline-block;
	width: 50px;
	height: 50px;
}

#container .video div.popupVid p.close a img {
	width: 100%;
}

/* 주요서비스 */

#container .serv ul {
	margin-bottom: 70px;
}

#container .serv ul li {
	display: inline-block;
	width: 23%;
	margin-bottom: 50px;
}

#container .serv ul li strong {
	display: inline-block;
	width: 100%;
	color: #444;
	font-size: 18px;
	letter-spacing: -1px;
	font-weight: 400;
	margin-top: 22px;
	position: relative;
}

/** footer **/

#footer {
	height: 200px;
	background-image: url(../images/footer_bg.jpg);
	background-size: cover;
}

#footer .wrapper {
	padding-top: 50px;
}

#footer p.contact {
	color: #bbb;
	font-size: 14px;
}

#footer p.contact span.add {
	display: inline-block;
	margin: 24px 0 10px;
}

#footer p.contact strong.tel {
	display: inline-block;
	width: 100%;
}

#footer ul.link {
	position: absolute;
	bottom: 0;
	right: 0;
	z-index: 3;
}

#footer ul.link>li {
	display: inline-block;
	transition: all 0.3s;
	font-size: 15px;
}

#footer ul.link>li h2 a {
	display: inline-block;
	margin-left: 3px;
	width: 160px;
	height: 25px;
	background-color: #6a6a6a;
	background-image: url(../images/link_bg.png);
	background-repeat: no-repeat;
	background-position: 140px center;
	box-sizing: border-box;
	color: #ccc;
	transition: all 0.3s;
	line-height: 25px;
	padding-left: 6px;
}

#footer ul.link>li:hover h2 a {
	color: #fff;
}

#footer ul.link>li ul.lst {
	position: absolute;
	bottom: 25px;
	right: 1px;
	display: none;
}

#footer ul.link>li ul.lst1 {
	right: 167px;
}

#footer ul.link>li ul.lst li a {
	display: block;
	width: 160px;
	height: 25px;
	line-height: 25px;
	background-color: #6a6a6a;
	transition: all 0.3s;
	padding-left: 3px;
	box-sizing: border-box;
	color: #ddd;
	/*font-size: 14px;*/
}

#footer ul.link>li ul.lst li a:hover {
	background-color: #333;
}

#footer ul.sns {
	position: absolute;
	top: 50px;
	right: 0;
}

#footer ul.sns li {
	display: inline-block;
	margin-left: 5px;
}

#footer ul.sns li a {
	text-align: center;
	display: inline-block;
	width: 38px;
	height: 38px;
	font-size: 23px;
	color: #222;
	background-color: #444;
	border-radius: 50%;
	transition: all 0.3s;
}

#footer ul.sns li a:hover {
	background-color: #999;
}

#footer ul.sns li a i {
	line-height: 38px;
}